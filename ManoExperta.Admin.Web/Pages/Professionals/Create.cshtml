@page
@model CreateModel
@{
    ViewData["Title"] = "Crear Profecional";
}


<h1 class="display-4 fw-bold">Profesionales</h1>
<p class="fs-5">Crear Nuevo</p>

<form method="post">
    <div class="form-group">
        <label asp-for="ProfessionalToCreate.UserName">Nombre de Usuario</label>
        <input asp-for="ProfessionalToCreate.UserName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="ProfessionalToCreate.FirstName">Nombre</label>
        <input asp-for="ProfessionalToCreate.FirstName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="ProfessionalToCreate.LastName">Apellido</label>
        <input asp-for="ProfessionalToCreate.LastName" class="form-control" />
    </div>

    <div class="form-group my-3">
        <label asp-for="ProfessionalToCreate.IsProfessional">Es Profesional</label>
        <input asp-for="ProfessionalToCreate.IsProfessional" type="checkbox" />
    </div>

    <div class="form-group">
        <label asp-for="ProfessionalToCreate.PhoneNumbers">Numero de Telefono</label>
        <input asp-for="ProfessionalToCreate.PhoneNumbers" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="ProfessionalToCreate.Emails">Email Primario</label>
        <input asp-for="ProfessionalToCreate.Emails" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="ProfessionalToCreate.ProfessionalCategoryCodes">Codigo de Categoria</label>
        <input asp-for="ProfessionalToCreate.ProfessionalCategoryCodes" class="form-control" />
    </div>
    <button type="submit" class="mt-3 btn btn-primary">Crear Nuevo</button>
</form>
